{
	"name": "ECharts",
	"version": "1.0",
	"author": [
		"[https://www.mediawiki.org/wiki/User:BertrandGorge Bertrand Gorge]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ECharts",
	"descriptionmsg": "ECharts-desc",
	"manifest_version": 1,
	"type": "parserhook",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"MessagesDirs": {
		"ECharts": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.mwecharts": {
			"scripts": [
				"mw.echarts.js"
			],
			"dependencies": [
				"jquery.ui",
				"ext.echarts",
				"ext.tika",
				"oojs-ui-core"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"messages": [
			]
		},
		"ext.echarts": {
			"localBasePath": "modules/ext.echarts",
			"remoteExtPath": "ECharts/modules/ext.echarts",
			"scripts": [
				"echarts.min.js"
			]
		},
		"ext.tika": {
			"localBasePath": "modules/ext.tika",
			"remoteExtPath": "ECharts/modules/ext.tika",
			"dependencies": [
				"ext.echarts"
			],
			"scripts": [
				"chart-render.js"
			],
			"styles": [
				"styles-rendering.css"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "ECharts/resources"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\ECharts\\": "includes/"
	},
	"APIModules": {

	},
	"HookHandlers": {
		"EChartsHooks": {
			"class": "MediaWiki\\Extension\\ECharts\\EChartsHooks"
		}
	},
	"Hooks": {
		"ParserFirstCallInit": "EChartsHooks"
	},
	"ExtensionMessagesFiles": {
		"EChartsMagic": "ECharts.i18n.magic.php"
	},
	"config": {
	}
}
